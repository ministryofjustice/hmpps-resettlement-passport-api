{
  "role": {
    "name": "hmpps-resettlement-passport-api"
  },
  "customDimensions": {
    "service.version": "${BUILD_NUMBER}"
  },
  "instrumentation": {
    "logging": {
      "level": "DEBUG"
    }
  },
  "selfDiagnostics": {
    "destination": "console"
  },
  "sampling": {
    "overrides": [
      {
        "telemetryType": "request",
        "attributes": [
          {
            "key": "http.url",
            "value": "https?://[^/]+/health.*",
            "matchType": "regexp"
          }
        ],
        "percentage": 10
      },
      {
        "telemetryType": "request",
        "attributes": [
          {
            "key": "http.url",
            "value": "https?://[^/]+/resettlement-passport.*",
            "matchType": "regexp"
          }
        ],
        "percentage": 100
      }
    ]
  }
}
