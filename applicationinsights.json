{
  "role": {
    "name": "hmpps-resettlement-passport-api"
  },
  "customDimensions": {
    "service.version": "${BUILD_NUMBER}"
  },
  "instrumentation": {
    "logging": {
      "level": "DEBUG"
    }
  },
  "selfDiagnostics": {
    "destination": "console",
    "level": "DEBUG"
  },
  "sampling": {
    "requestsPerSecond": 5.0,
    "overrides": [
      {
        "percentage": 10,
        "telemetryType": "request",
        "attributes": [
          {
            "key": "http.url",
            "value": "https?://[^/]+/health.*",
            "matchType": "regexp"
          }
        ]
      }
    ]
  }
}
